{
  "name": "overpowered-monorepo",
  "description": "A monorepo with two next.js apps and an express api",
  "license": "MIT",
  "keywords": [
    "monorepo",
    "next.js",
    "express.js",
    "tailwind",
    "shadcn"
  ],
  "files": [
    "apps",
    "packages",
    "eslintrc.js",
    ".gitignore",
    ".npmrc",
    ".components.json",
    "LICENSE.md",
    "package-lock.json",
    "package.json",
    "README.md",
    "tsconfig.json",
    "turbo.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ashishrout-tech/monorepo.git"
  },
  "author": "ashishrout-tech",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "clean": "turbo daemon clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@overpowered-monorepo/eslint-config-custom": "*",
    "@turbo/gen": "^1.10.12",
    "eslint": "^8.46.0",
    "prettier": "^3.0.1",
    "turbo": "latest"
  },
  "packageManager": "npm@9.7.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@types/inquirer": "^9.0.3",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "glob": "^10.3.3",
    "mongoose": "^7.4.2",
    "saslprep": "^1.0.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6"
  }
}
